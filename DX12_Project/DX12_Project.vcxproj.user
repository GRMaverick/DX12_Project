<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|x64'">
    <LocalDebuggerWorkingDirectory>$(SolutionDir)$(Platform)\$(Configuration)\</LocalDebuggerWorkingDirectory>
    <DebuggerFlavor>WindowsLocalDebugger</DebuggerFlavor>
    <LocalDebuggerCommandArguments>/d3ddebug /Width:1920 /Height:1080</LocalDebuggerCommandArguments>
    <RemoteDebuggerCommandArguments>/d3ddebug /Width:1920 /Height:1080</RemoteDebuggerCommandArguments>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|x64'">
    <LocalDebuggerCommandArguments>/Height:1080 /Width:1920</LocalDebuggerCommandArguments>
    <RemoteDebuggerCommandArguments>/Height:1080 /Width:1920</RemoteDebuggerCommandArguments>
  </PropertyGroup>
</Project>